<reactiveUi:ReactiveUserControl x:TypeArguments="viewModels:SecondViewModel" x:Class="ReactiveUI.Samples.Routing.Views.SecondView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ReactiveUI.Samples.Routing.Views"
             xmlns:reactiveUi="http://reactiveui.net"
             xmlns:viewModels="clr-namespace:ReactiveUI.Samples.Routing.ViewModels"
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"                                    
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Button Content="Back" Height="50" x:Name="backButton" />
        <telerik:RadVirtualGrid x:Name="VirtualGrid" Height="102"
                                CanUserEdit="False"
                                InitialRowCount="10"                                
                                InitialColumnCount="10"
                                MeasureTextOnRender="True"/>
    </StackPanel>
</reactiveUi:ReactiveUserControl>
